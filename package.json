{
  "name": "react-dom-core",
  "version": "0.0.2",
  "description": "Exposes the core/internal modules from react-dom lib.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "prepublish": "npm run clean && npm run copy",
    "clean": "rm -rf lib",
    "copy": "cp -r node_modules/react-dom/lib/. lib",
    "changelog": "conventional-changelog -p angular -o CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/react-dom-core"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/react-dom-core/issues"
  },
  "keywords": [
    "react",
    "react-dom",
    "react-dom-core"
  ],
  "files": [
    "lib"
  ],
  "optionalDependencies": {
    "fbjs": "*",
    "object-assign": "*",
    "react": "*"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.3",
    "react-dom": "^15"
  },
  "license": "MIT"
}
